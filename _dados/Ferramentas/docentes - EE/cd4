<script type="text/javascript">
    $(document).ready(function(){
        $("dd").slideUp("slow");
        $("dd").hide();
        $("dt a").click(function(){
            
            $('.detalhe_com_submenu').removeClass('detalhe_com_submenu_menos'); 
            $(this).prev().addClass("detalhe_com_submenu_menos");
            $("dd:visible").slideUp("slow");
            $(this).parent().next().slideDown("slow");   
            
            if($(this).parent().next().get(0).tagName=="DD"){
               return false;
            }
            
        });
        
        
        /*
        for (var i in location){
		//navego pelas chaves do array como um for
		console.log('Campo \''+i+'\': ' + location[i] );
	}*/
        
        
        $('.menu_institucional a').each(function(){
            
            var vhref = $(this).attr('href').toString();
            var vloca = location.pathname.toString().replace("index/","");
            var vloca2 = location.pathname.toString();
            
            //console.log( vhref + " - " + location.pathname.toString() );           
            
            
            if(vhref.substr(-1)== "/"){
                vhref = vhref.substr(0,vhref.length-1);
            }
            
            if(vloca.substr(-1)== "/"){
                vloca = vloca.substr(0,vloca.length-1);
            }
            
            if(vloca2.substr(-1)== "/"){
                vloca2 = vloca2.substr(0,vloca2.length-1);
            }
            
            if( (vhref ==vloca) || (vhref ==vloca2)  ){
                
               // console.log( vhref + " - " + location.pathname.toString() );               
                
                
                if( $(this).parent().get(0).tagName == "LI"){
                    $(this).parent().parent().parent().prev().children('a').click();
                    $(this).addClass('ativo_sub');
                }else{
                    $(this).addClass('ativo_menu_lat');
                }
            }
        });
        
        
        console.log();
        
    });
</script>
